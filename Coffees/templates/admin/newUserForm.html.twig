{% extends 'base.html.twig' %}

{% block title %} New user {% endblock %}

{% block main %}

<h1>New User</h1>

<form action="/" method="post">
    <input type="hidden" name="action" value="processNewUser">
    <input type="hidden" name="module" value="admin">

    <p>
        Username:
        <input name="username">
    </p>
    <p>
        Password:
        <input name="password">
    </p>
        <input type="submit">
</form>

{% endblock %}